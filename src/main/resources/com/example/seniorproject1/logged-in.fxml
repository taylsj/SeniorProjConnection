<AnchorPane prefHeight="638.0" prefWidth="851.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seniorproject1.LoggedInPageController">
   <children>
      <AnchorPane prefHeight="638.0" prefWidth="224.0" style="-fx-background-color: #311432;">
         <children>
            <ImageView fitHeight="224.0" fitWidth="224.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@profile1.png" />
               </image>
            </ImageView>
            <Label fx:id="userNameProfileLabel" alignment="CENTER" layoutX="18.0" layoutY="244.0" prefHeight="38.0" prefWidth="183.0" text="AB" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Label>
            <Button fx:id="switchToProfileButton" layoutY="334.0" mnemonicParsing="false" onAction="#switchToProfileOnAction" prefHeight="25.0" prefWidth="226.0" style="-fx-background-color: #702963;" text="Profile" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="switchToPostButton" layoutY="387.0" mnemonicParsing="false" onAction="#switchToPostOnAction" prefHeight="25.0" prefWidth="224.0" style="-fx-background-color: #702963;" text="Post" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="switchToSearchButton" layoutX="1.0" layoutY="448.0" mnemonicParsing="false" onAction="#switchToSearchOnAction" prefHeight="25.0" prefWidth="224.0" style="-fx-background-color: #702963;" text="Search" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Hyperlink fx:id="signOutLink" layoutX="75.0" layoutY="515.0" onAction="#signOutLinkOnAction" text="Sign Out">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Hyperlink>
         </children>
      </AnchorPane>
      <ListView fx:id="homePostListView" layoutX="244.0" layoutY="64.0" prefHeight="289.0" prefWidth="596.0" />
      <Label layoutX="265.0" layoutY="32.0" prefHeight="17.0" prefWidth="237.0" text="Your Posts:" />
      <Label layoutX="251.0" layoutY="400.0" prefHeight="17.0" prefWidth="114.0" text="Most Posted Game:" />
      <Label layoutX="251.0" layoutY="427.0" prefHeight="17.0" prefWidth="130.0" text="Most Posted Platform:" />
      <Label fx:id="gameMessageLabel" layoutX="381.0" layoutY="400.0" prefHeight="17.0" prefWidth="169.0" text="Label" />
      <Label fx:id="platformMessageLabel" layoutX="381.0" layoutY="427.0" prefHeight="17.0" prefWidth="169.0" text="Label" />
   </children>
